generator client {
  provider = "prisma-client-js"
  output = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  // url      = env("DATABASE_URL")
  url = "postgresql://admin:admin1234@localhost:5434/freelance_ai"
}


model UserSchema{
  user_id Int @id @default(autoincrement())
  Name String
  Email String @unique
  Password String
  Experience Int
  Rating Int
  Skills String[]
}